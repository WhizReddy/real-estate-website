{
  "name": "real-estate-website",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:fast": "next dev --turbo --experimental-https",
    "postinstall": "prisma generate",
    "prebuild": "echo 'Skipping checks for Vercel'",
    "build": "prisma generate && next build",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "build:analyze": "ANALYZE=true next build",
    "build:production": "NODE_ENV=production npm run build",
    "build:staging": "NODE_ENV=staging npm run build",
    "start": "next start",
    "start:production": "NODE_ENV=production next start -p 3000",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:comprehensive": "node src/__tests__/test-runner.js",
    "test:accessibility": "jest --testPathPattern=accessibility",
    "test:integration": "jest --testPathPattern=integration",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma db push --force-reset && npm run db:seed",
    "create-admin": "node scripts/create-admin.js",
    "fix-database": "node scripts/fix-database.js",
    "check-db": "node scripts/check-db.js",
    "gentle-fix": "node scripts/gentle-db-fix.js",
    "fix-schema": "node scripts/fix-schema-only.js",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@prisma/client": "^5.22.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/leaflet": "^1.9.20",
    "@vercel/blob": "^1.1.1",
    "@vercel/postgres": "^0.10.0",
    "bcryptjs": "^3.0.2",
    "critters": "^0.0.23",
    "dotenv": "^17.2.3",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.525.0",
    "next": "15.4.10",
    "next-auth": "^4.24.11",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.60.0",
    "react-leaflet": "^5.0.0",
    "web-vitals": "^5.0.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.15",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.1",
    "jest": "^30.0.5",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.0.5",
    "prisma": "^5.22.0",
    "tailwindcss": "^4.1.15",
    "tsx": "^4.7.0",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
